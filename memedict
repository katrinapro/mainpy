meme_dict = {
    "КРИНЖ": "Что-то очень странное или стыдное",
    "ЛОЛ": "Что-то очень смешное",
    "РОФЛ": "Шутка",
    "ЩИЩ": "Одобрение или восторг",
    "КРИПОВЫЙ": "Страшный, пугающий",
    "АГРИТЬСЯ": "Злиться"
}

def main():
    print("Добро пожаловать в словарь сленговых слов!")
    print("Введите слово (большими буквами), чтобы получить его значение.")
    print("Вы можете ввести до 5 слов за раз. Для выхода введите 'ВЫХОД'.")

    for _ in range(5):
        word = input("Введите непонятное слово: ")
        if word == 'ВЫХОД':
            print("Спасибо за использование словаря! Пока!")
            break
        elif word in meme_dict:
            print({word},{meme_dict[word]})
        else:
            print("Извините, это слово не найдено в словаре.")

if __name__ == "__main__":
    main()
